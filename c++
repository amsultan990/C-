#include<iostream>

using namespace std;
int main(){
    // Anding two Integers.
    
    //Declaring Integers. 
    int a;
    int b;
    
    // Asking for Inputs(Integers)
    cout<<"please enter a number:a ";
    cin>>a;
    cout<<"please enter another number:b ";
    cin>>b;
    
    //Addition of nmbers
    int sum = a+b;
    cout<<"Addition of " <<a<< " and " <<b << " is equal to "  <<sum<<endl;
    return 0;
}
